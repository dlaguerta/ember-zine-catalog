<h1> Current Zine Catalog</h1>
<h6> Start a quick search by content tags in the search bar below! </h6>
{{#list-filter
   filter=(action 'filterByTags')
   as |zines|}}
  <ul class="results row small-up-1 medium-up-2 large-up-3">
    {{#each zines as |zineItem|}}
      <li class="column">{{zine-listing zine=zineItem}}</li>
    {{/each}}
  </ul>
{{/list-filter}}


{{outlet}}
